OBJS	= airplane.o Q1.o Q2.o Q3.o Utillities.o full_schecdule.o arrivals.o
SOURCE	= airplane.c Q1.c Q2.c Q3.c Utillities.c full_schecdule.c arrivals.c
HEADER	= Q1.h Q2.h Q3.h Utillities.h
OUT	= a.out
CC	 = gcc
FLAGS	 = -g -c -Wall
LFLAGS	 = 

all: $(OBJS)
	$(CC) -g $(OBJS) -o $(OUT) $(LFLAGS)

airplane.o: airplane.c
	$(CC) $(FLAGS) airplane.c -std=c++17

Q1.o: Q1.c
	$(CC) $(FLAGS) Q1.c -std=c++17

Q2.o: Q2.c
	$(CC) $(FLAGS) Q2.c -std=c++17

Q3.o: Q3.c
	$(CC) $(FLAGS) Q3.c -std=c++17

Utillities.o: Utillities.c
	$(CC) $(FLAGS) Utillities.c -std=c++17

full_schecdule.o: full_schecdule.c
	$(CC) $(FLAGS) full_schecdule.c -std=c++17

arrivals.o: arrivals.c
	$(CC) $(FLAGS) arrivals.c -std=c++17


clean:
	rm -f $(OBJS) $(OUT)
